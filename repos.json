[
  {
    "kernelSource": {
      "name": "Tejas-Kernel-Vayu-Sixteen",
      "repo": "https://github.com/tejas101k/kernel_xiaomi_vayu",
      "branch": "sixteen",
      "device": "vayu"
    },
    "withKernelSU": true,
    "toolchains": [
